{
    "name": "timing",
    "displayName": "Timing",
    "description": "Convert timestamps in different formats.",
    "version": "0.0.1",
    "publisher": "HaaLeo",
    "engines": {
        "vscode": "^1.23.0"
    },
    "preview": true,
    "qna": "https://github.com/HaaLeo/vscode-timing/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/HaaLeo/vscode-timing"
    },
    "bugs": {
        "url": "https://github.com/HaaLeo/vscode-timing/issues"
    },
    "author": {
        "name": "Leo Hanisch"
    },
    "galleryBanner": {
        "color": "#252627",
        "theme": "dark"
    },
    "icon": "doc/Icon_128x128.png",
    "readme": "README.md",
    "categories": [
        "Other",
        "Formatters"
    ],
    "keywords": [
        "converter", "time", "epoch", "ISO", "8601"
    ],
    "activationEvents": [
        "onCommand:timing.convertTime"
    ],
    "main": "./out/timing",
    "contributes": {
        "commands": [
            {
                "command": "timing.convertTime",
                "title": "Convert time to UTC"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
