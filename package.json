{
    "name": "timing",
    "displayName": "Timing",
    "description": "Converts and visualizes timestamps in different formats.",
    "version": "0.1.2",
    "publisher": "HaaLeo",
    "engines": {
        "vscode": "^1.23.0"
    },
    "license": "MIT",
    "preview": true,
    "qna": "https://github.com/HaaLeo/vscode-timing/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/HaaLeo/vscode-timing"
    },
    "bugs": {
        "url": "https://github.com/HaaLeo/vscode-timing/issues"
    },
    "author": {
        "name": "Leo Hanisch",
        "url": "https://github.com/HaaLeo"
    },
    "galleryBanner": {
        "color": "#252627",
        "theme": "dark"
    },
    "icon": "doc/Icon_128x128.png",
    "readme": "README.md",
    "categories": [
        "Other",
        "Formatters"
    ],
    "keywords": [
        "converter",
        "time",
        "epoch",
        "ISO 8601",
        "unix"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/timing",
    "contributes": {
        "commands": [
            {
                "command": "timing.convertTime",
                "title": "Timing: Convert time to UTC \n(DEPRECATED. Use timing.epochToIsoUtc instead)"
            },
            {
                "command": "timing.epochToIsoUtc",
                "title": "Timing: Convert epoch time to ISO format (UTC)"
            },
            {
                "command": "timing.epochToIsoLocal",
                "title": "Timing: Convert epoch time to ISO format (Local)"
            },
            {
                "command": "timing.isoRfcToEpoch",
                "title": "Timing: Convert ISO 8601 or RFC 2822 date to epoch time"
            }

        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "integrationtest": "node ./node_modules/vscode/bin/test",
        "unittest": "mocha out/test/unittest/**/*.js",
        "tslint": "tslint --project ./"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.0",
        "@types/node": "^10.1.3",
        "@types/sinon": "^5.0.1",
        "mocha": "^5.2.0",
        "sinon": "^5.0.10",
        "tslint": "^5.10.0",
        "typescript": "^2.8.3",
        "vscode": "^1.1.18"
    }
}
