{
    "name": "timing",
    "displayName": "Time Converter",
    "description": "Converts and visualizes timestamps in various formats.",
    "version": "0.3.0",
    "publisher": "HaaLeo",
    "engines": {
        "vscode": "^1.23.0"
    },
    "license": "MIT",
    "qna": "https://github.com/HaaLeo/vscode-timing/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/HaaLeo/vscode-timing"
    },
    "bugs": {
        "url": "https://github.com/HaaLeo/vscode-timing/issues"
    },
    "author": {
        "name": "Leo Hanisch",
        "url": "https://github.com/HaaLeo"
    },
    "galleryBanner": {
        "color": "#252627",
        "theme": "dark"
    },
    "icon": "doc/Icon_128x128.png",
    "readme": "README.md",
    "categories": [
        "Other",
        "Formatters"
    ],
    "keywords": [
        "converter",
        "time",
        "epoch",
        "ISO 8601",
        "unix"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/timing",
    "contributes": {
        "commands": [
            {
                "command": "timing.convertTime",
                "category": "Timing",
                "title": "Convert time to UTC (DEPRECATED. Use timing.epochToIsoUtc instead)"
            },
            {
                "command": "timing.epochToIsoUtc",
                "category": "Timing",
                "title": "Convert epoch time to ISO 8601 format (UTC)"
            },
            {
                "command": "timing.epochToIsoLocal",
                "category": "Timing",
                "title": "Convert epoch time to ISO 8601 format (Local)"
            },
            {
                "command": "timing.isoRfcToEpoch",
                "category": "Timing",
                "title": "Convert ISO 8601 or RFC 2822 time to epoch format"
            },
            {
                "command": "timing.nowAsEpoch",
                "category": "Timing",
                "title": "Get current time as epoch format"
            },
            {
                "command": "timing.nowAsIsoUtc",
                "category": "Timing",
                "title": "Get current time as ISO UTC format"
            },
            {
                "command": "timing.nowAsIsoLocal",
                "category": "Timing",
                "title": "Get current time as ISO Local format"
            },
            {
                "command": "timing.customToEpoch",
                "category": "Timing",
                "title": "Convert custom time to epoch format."
            }
        ],
        "configuration": {
            "type": "object",
            "title": "The timing configuration.",
            "properties": {
                "timing.customFormats": {
                    "title": "Custom time formats.",
                    "description": "An array of custom source/target formats used to convert from/to.",
                    "type": [
                        "array"
                    ],
                    "items": {
                        "title": "Custom time formats.",
                        "description": "Custom time format to convert from/to.",
                        "type": "object",
                        "properties": {
                            "format": {
                                "type": "string",
                                "title": "The custom time format.",
                                "description": "A custom momentjs format to convert a time from/to."
                            },
                            "description": {
                                "type": "string",
                                "title": "The description.",
                                "description": "The description of the format. Used as quick pick description."
                            },
                            "detail": {
                                "type": "string",
                                "title": "The detail.",
                                "description": "The detail of the format. Used as quick pick detail."
                            }
                        },
                        "required": [
                            "format"
                        ]
                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "integrationtest": "node ./node_modules/vscode/bin/test",
        "unittest": "mocha out/test/unittest/**/*.js",
        "tslint": "tslint --project ./"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.0",
        "@types/node": "^10.1.3",
        "@types/sinon": "^5.0.1",
        "mocha": "^5.2.0",
        "sinon": "^5.0.10",
        "tslint": "^5.10.0",
        "typescript": "^2.8.3",
        "vscode": "^1.1.18"
    },
    "dependencies": {
        "moment": "^2.22.2"
    }
}
